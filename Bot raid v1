import discord
from discord.ext import commands


Token = input("Inserez le token ici : ")


bot = commands.Bot(command_prefix='!')


client = commands.Bot(command_prefix='!')


bot.remove_command('help')


@bot.event
async def on_ready():
    print("----------------------------")
    print(f"Bot Status: activ√©!...")
    print("----------------------------")
    print("!spam pour spam 50 msg")
    print("!nuke pour nuke")
    print("!name pour changer le nom du serv")
    print("----------------------------")
    print("Youtube : https://www.youtube.com/channel/UCE5U7kWWiYzQ3HgQxTQ_otg/featured")
    print("----------------------------")


